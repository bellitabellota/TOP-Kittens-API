<%= form_with model:@kitten do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :age%>
  <%= form.range_field :age %>

  <%= form.label :cuteness %>
  <%= form.text_field :cuteness %>

  <%= form.label :softess %>
  <%= form.text_field :softess %>

  <%= form.submit "Add kitten" %>
<% end %>