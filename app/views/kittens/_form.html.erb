<%= form_with model: kitten do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :age%>
  <%= form.text_field :age %>

  <%= form.label :cuteness %>
  <%= form.text_field :cuteness %>

  <%= form.label :softness %>
  <%= form.text_field :softness %>

  <%= form.submit "Submit" %>
<% end %>